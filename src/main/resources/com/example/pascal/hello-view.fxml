<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pascal.PascalCon">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
      <children>
         <Button minWidth="62.0" mnemonicParsing="false" onAction="#onButtonClick" text="Generuj" />
          <TextField fx:id="Pole" minWidth="44.0" onAction="#onButtonClick" promptText="Podaj ilość wierszy">
            <HBox.margin>
               <Insets left="10.0" />
            </HBox.margin></TextField>
      </children>
   </HBox>
   <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="322.0" prefWidth="360.0">
      <content>
         <TextFlow fx:id="displayed" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" textAlignment="CENTER">
            <padding>
               <Insets top="10.0" />
            </padding>
         </TextFlow>
      </content>
   </ScrollPane>
</VBox>
